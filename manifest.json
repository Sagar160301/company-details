{
    "manifest_version": 3,
    "name": "Gmail Rejection",
    "version": "1.0",
    "description": "Gmb email rejection",
    "permissions": ["tabs", "activeTab", "webNavigation", "scripting", "alarms"],
    "background": {
      "service_worker": "background.js"
    },
    "content_scripts": [
      {
        "matches": ["https://www.companydetails.in/*"],
        "js": ["content.js"],
        "run_at": "document_idle"
      }
    ]
  }